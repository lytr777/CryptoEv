{
  "algorithm": {
    "type": "tabu",
    "update_count": 100,
    "comparator": {
      "name": "max_min"
    },
    "stop_condition": {
      "name": "mf_calls",
      "limit": 1000
    }
  },
  "predictive_function": {
    "type": "ibs",
    "key_generator": {
      "name": "a5_1"
    },
    "selection": {
      "name": "const",
      "value": 1000
    },
    "corrector": {
      "name": "mass"
    }
  },
  "solvers": {
    "type": "net",
    "init_solver": {
      "name": "lingeling",
      "tag": "init",
      "tl_util": false,
      "tl": 0,
      "workers": 1,
      "attempts": 5,
      "simplify": false
    },
    "main_solver": {
      "name": "lingeling",
      "tag": "main",
      "tl_util": false,
      "tl": 5,
      "workers": 1,
      "attempts": 5,
      "simplify": true
    }
  },
  "concurrency": {
    "type": "workers",
    "thread_count": 32,
    "task_queue": {
      "name": "generate"
    }
  },
  "output": {
    "type": "storage",
    "log_name": "log",
    "debug_name": "debug",
    "src_dump": [
      "./configuration/options.py",
      "./predictive_function/module/selection.py",
      "./predictive_function/module/corrector.py",
      "./util/comparator.py"
    ]
  }
}

