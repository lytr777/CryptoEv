{
  "predictive_function": {
    "type": "ibs",
    "chunk_size": 500,
    "key_generator": {
      "name": "asg_72_76"
    },
    "selection": {
      "name": "const",
      "value": 10000
    },
    "corrector": {
      "name": "mass",
      "coef": 0.1,
      "min_tl": 0.001
    }
  },
  "solvers": {
    "type": "net",
    "init_solver": {
      "name": "lingeling",
      "tag": "init",
      "tl_util": false,
      "tl": 0,
      "workers": 1,
      "attempts": 5,
      "simplify": false
    },
    "main_solver": {
      "name": "lingeling",
      "tag": "main",
      "tl_util": true,
      "tl": 30,
      "workers": 1,
      "attempts": 5,
      "simplify": true
    }
  },
  "concurrency": {
    "type": "apply_pool",
    "thread_count": 36,
    "task_queue": {
      "name": "task"
    }
  },
  "output": {
    "type": "storage",
    "base_path": "./output/_true_logs",
    "log_name": "log",
    "debug_name": "debug",
    "src_dump": [
      "./configuration/options.py",
      "./predictive_function/module/selection.py",
      "./predictive_function/module/corrector.py",
      "./algorithm/module/comparator.py"
    ]
  }
}
