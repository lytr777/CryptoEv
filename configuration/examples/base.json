{
  "algorithm": {
    "type": "evolution",
    "strategy": {
      "name": "plus",
      "mu": 1,
      "lambda": 1
    },
    "mutation_function": {
      "name": "uniform",
      "scale": 1.0
    },
    "crossover_function": {
      "name": "one-point"
    },
    "stagnation_limit": 200,
    "comparator": {
      "name": "max_min"
    },
    "stop_condition": {
      "name": "time",
      "limit": "18:50:00"
    }
  },
  "predictive_function": {
    "type": "ibs",
    "key_generator": {
      "name": "asg_96_112"
    },
    "selection": {
      "name": "function",
      "value": 1000,
      "min_N": 300,
      "max_N": 1000
    },
    "corrector": {
      "name": "mass",
      "coef": 0.1,
      "lower_bound": 0.001
    }
  },
  "solvers": {
    "type": "net",
    "init_solver": {
      "name": "lingeling",
      "tag": "init",
      "tl_util": false,
      "tl": 0,
      "workers": 1,
      "attempts": 5,
      "simplify": false
    },
    "main_solver": {
      "name": "lingeling",
      "tag": "main",
      "tl_util": false,
      "tl": 30,
      "workers": 1,
      "attempts": 5,
      "simplify": true
    }
  },
  "concurrency": {
    "type": "apply_pool",
    "thread_count": 36,
    "task_queue": {
      "name": "generate"
    }
  },
  "output": {
    "type": "storage",
    "log_name": "log",
    "debug_name": "debug",
    "src_dump": [
      "./configuration/options.py",
      "./algorithm/module/mutation.py",
      "./algorithm/module/crossover.py",
      "./predictive_function/module/selection.py",
      "./predictive_function/module/corrector.py",
      "./algorithm/module/comparator.py"
    ]
  }
}
