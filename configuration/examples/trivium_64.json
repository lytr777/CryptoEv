{
  "algorithm": {
    "type": "evolution",
    "strategy": {
      "name": "plus",
      "mu": 1,
      "lambda": 1
    },
    "mutation_function": {
      "name": "uniform",
      "scale": 1.0
    },
    "crossover_function": {
      "name": "one-point"
    },
    "stagnation_limit": 100,
    "comparator": {
      "name": "max_min"
    },
    "stop_condition": {
      "name": "locals",
      "limit": 1
    }
  },
  "predictive_function": {
    "type": "ibs",
    "key_generator": {
      "name": "trivium_64"
    },
    "selection": {
      "name": "const",
      "value": 500
    },
    "corrector": {
      "name": "mass",
      "coef": 0.1,
      "lower_bound": 0.001
    }
  },
  "solvers": {
    "type": "net",
    "init_solver": {
      "name": "lingeling",
      "tag": "init",
      "tl_util": false,
      "tl": 0,
      "workers": 1,
      "attempts": 5,
      "simplify": false
    },
    "main_solver": {
      "name": "lingeling",
      "tag": "main",
      "tl_util": false,
      "tl": 5,
      "workers": 1,
      "attempts": 5,
      "simplify": true
    }
  },
  "concurrency": {
    "type": "workers",
    "thread_count": 32,
    "task_queue": {
      "name": "generate"
    }
  },
  "output": {
    "type": "storage",
    "log_name": "log",
    "debug_name": "debug",
    "src_dump": [
      "./configuration/options.py",
      "./algorithm/module/mutation.py",
      "./algorithm/module/crossover.py",
      "./predictive_function/module/selection.py",
      "./predictive_function/module/corrector.py",
      "./algorithm/module/comparator.py"
    ]
  }
}
